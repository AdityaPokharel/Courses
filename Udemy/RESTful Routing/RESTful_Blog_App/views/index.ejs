<% include ./partials/header.ejs %>
<h1>Hello World</h1>

<% blogs.forEach((blog) => { %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" width="400px"> <br>
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
    </div>
<% }) %>

